<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tobe.mes.tobesystem.Mapper.Scm.Standard.SCMPart_Mapper">
    <select id="bPart_get" resultType="com.tobe.mes.tobesystem.Bean.MESBean.SYS_BPART_CD.SYS_BPART_CD">
        CALL TOBE_MES.SP_SYS_BPART_GET(#{page_num},#{total_num},'',#{keyword},#{keyword2},'S')
    </select>
    <select id="bPart_get_count" resultType="Integer">
         CALL TOBE_MES.SP_SYS_BPART_GET(#{page_num},#{total_num},'',#{keyword},#{keyword2},'C')
    </select>
    <select id="cargo_cd_au" resultType="com.tobe.mes.tobesystem.Bean.Result">
        CALL TOBE_MES.SP_SYS_CARGO_ADD(#{cargo_code},#{cargo_name},#{cargo_grp_code},#{qty_yn},#{use_yn},#{user_code},#{keyword})
    </select>

    <select id="cargo_cd_delete" resultType="com.tobe.mes.tobesystem.Bean.Result">
        CALL TOBE_MES.SP_SYS_CARGO_DEL(#{code})
    </select>
    <select id="scmBPrice_excel_download" resultType="com.tobe.mes.tobesystem.Bean.MESBean.SYS_BPRICE_CD.SYS_BPRICE_CD">
        SELECT * FROM SYS_BPRICE_CD
    </select>

</mapper>
