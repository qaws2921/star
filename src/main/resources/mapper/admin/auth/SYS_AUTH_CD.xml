<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tobe.mes.tobesystem.Mapper.Admin.Auth.SYS_AUTH_CD_Mapper">
    <select id="auth_cd_get" resultType="com.tobe.mes.tobesystem.ADMIN.AUTH.SYS_AUTH_CD.SYS_AUTH_CD">
        CALL TOBE_MES.SP_SYS_AUTH_GROUP_GET(#{page_num},#{total_num},'','S')
    </select>
    <select id="auth_cd_count" resultType="Integer">
        CALL TOBE_MES.SP_SYS_AUTH_GROUP_GET(#{page_num},#{total_num},'','C')
    </select>
    <insert id="auth_cd_au">
        CALL TOBE_MES.SP_SYS_AUTH_GROUP_ADD(#{auth_code},#{auth_name},#{keyword})
    </insert>
    <delete id="auth_cd_delete">
        CALL TOBE_MES.SP_SYS_AUTH_GROUP_DEL(#{code})
    </delete>


</mapper>
