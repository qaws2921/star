<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tobe.mes.tobesystem.Mapper.Admin.Auth.SYS_MENU_CD_Mapper">
    <select id="menu_group_get" resultType="com.tobe.mes.tobesystem.ADMIN.AUTH.SYS_MENU_CD.SYS_MENU_CD">
        CALL TOBE_MES.SP_SYS_MENU_GROUP_GET()
    </select>
    <select id="menu_cd_program_get" resultType="com.tobe.mes.tobesystem.ADMIN.AUTH.SYS_MENU_CD.SYS_MENU_CD">
         CALL TOBE_MES.SP_SYS_AUTH_PROGRAM_GET(#{menu_code},#{auth_code})
    </select>
    <insert id="auth_program_add">
        CALL TOBE_MES.SP_SYS_AUTH_PROGRAM_ADD(#{auth_code},#{menu_code},#{check_get},#{check_add},#{check_edit},#{check_del})
    </insert>


</mapper>
