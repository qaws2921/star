<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tobe.mes.tobesystem.Mapper.SYS_COMMON_Mapper">
    <select id="common_get" resultType="com.tobe.mes.tobesystem.SYS_COMMON.SYS_COMMON">
        CALL TOBE_MES.SP_SYS_COMMON_GET(#{page_num},#{total_num},#{keyword},'','S')
    </select>
    <select id="common_count" resultType="Integer">
        CALL TOBE_MES.SP_SYS_COMMON_GET(#{page_num},#{total_num},#{keyword},'','')
    </select>
    <select id="common_group_get" resultType="com.tobe.mes.tobesystem.SYS_COMMON.SYS_COMMON">
        CALL TOBE_MES.SP_SYS_COMMON_GROUP_GET(0,0,"S")
    </select>

</mapper>
